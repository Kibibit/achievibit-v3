{
  "name": "@kibibit/achievibit-backend",
  "version": "0.0.2-beta.19",
  "description": "Achievements & Gamification for Cloud Git Systems",
  "author": "thatkookooguy <neilkalman@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "if [ ! -d 404 ]; then npm run fetch-404; fi",
    "fetch-404": "rimraf 404 && degit https://github.com/kibibit/404 404",
    "build:sdk": "swagger-typescript-api -p http://localhost:10102/api/docs-json -o ../sdk/src/generated --extract-enums --module-name-first-tag --modular --axios && cd ../sdk && npm version minor",
    "generate-barrels": "barrelsby --delete -d ./src -l below -q --exclude spec.ts --exclude spec.js --exclude __mocks__ && echo '> Barrels Generated'",
    "prebuild": "npm run generate-barrels",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:styles": "stylelint \"projects/design-system/src/**/*.scss\" --config .stylelintrc.js",
    "lint:styles:fix": "stylelint \"projects/design-system/src/**/*.scss\" --config .stylelintrc.js --fix",
    "pretest": "rimraf ../test-results/server",
    "test": "jest --cache",
    "pretest:watch": "rimraf ../test-results/server",
    "test:watch": "node scripts/test-watch.js",
    "pretest:coverage": "rimraf ../test-results/server",
    "test:coverage": "jest --coverage --cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "migration:generate": "ts-node scripts/migration-generate.ts",
    "migration:create": "ts-node scripts/migration-create.ts",
    "toDockerEnv": "export NODE_ENV=docker && ts-node ./scripts/to-env.ts"
  },
  "dependencies": {
    "@gitbeaker/rest": "^40.3.0",
    "@kibibit/achievements": "link:../achievements",
    "@kibibit/configit": "^2.11.1",
    "@kibibit/pretty-stack": "^0.4.3",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.6",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.6",
    "@octokit/auth-app": "^7.1.1",
    "@octokit/core": "^5.2.0",
    "@octokit/rest": "^21.1.1",
    "@socket.io/admin-ui": "^0.5.1",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.7.7",
    "badge-maker": "^4.0.0",
    "bitbucket": "^2.12.0",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cookie": "^1.0.2",
    "express-session": "^1.18.0",
    "fs-extra": "^11.2.0",
    "http-status-codes": "^2.3.0",
    "ibm-openapi-validator": "^1.35.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.7",
    "nestjs-asyncapi": "^1.3.0",
    "openai": "^4.90.0",
    "passport-bitbucket-oauth2": "^0.1.2",
    "passport-github2": "^0.1.12",
    "passport-gitlab2": "^5.0.0",
    "passport-jwt": "^4.0.1",
    "passport": "^0.7.0",
    "pg": "^8.13.0",
    "prettyjson": "^1.2.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "smee-client": "^2.0.3",
    "socket.io": "^4.8.0",
    "timezones.json": "^1.7.1",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin-template": "^18.3.1",
    "@angular-eslint/eslint-plugin": "^18.3.1",
    "@angular-eslint/template-parser": "^18.3.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@octokit/webhooks-types": "^7.5.1",
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express-session": "^1.18.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/ibm-openapi-validator": "^0.15.4",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.16.11",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/prettyjson": "^0.0.33",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "barrelsby": "^2.8.1",
    "commitizen": "^4.3.0",
    "degit": "^2.8.4",
    "dotenv": "^16.4.5",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint": "^9.27.0",
    "husky": "^9.1.6",
    "jest-junit": "^16.0.0",
    "jest-stare": "^2.5.2",
    "jest": "^29.5.0",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-typescript-api": "^13.0.23",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.7.0",
    "typeorm-cli": "^1.0.7",
    "typescript": "^5.6.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setup-globals.ts"
    ],
    "globalTeardown": "<rootDir>/../test-tools/test-teardown.ts",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePathIgnorePatterns": [
      "node_modules"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "maxWorkers": "75%",
    "reporters": [
      "default",
      [
        "jest-stare",
        {
          "resultDir": "../test-results/server",
          "reportTitle": "jest-stare!",
          "additionalResultsProcessors": [
            "jest-junit"
          ],
          "coverageLink": "./coverage/lcov-report/index.html"
        }
      ],
      [
        "jest-junit",
        {
          "outputDirectory": "../test-results/server",
          "outputName": "junit.xml"
        }
    ]
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "coverageDirectory": "../../test-results/server/coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@kb-auth$": "<rootDir>/auth/index",
      "^@kb-config$": "<rootDir>/config/index",
      "^@kb-decorators$": "<rootDir>/decorators/index",
      "^@kb-filters$": "<rootDir>/filters/index",
      "^@kb-guards$": "<rootDir>/guards/index",
      "^@kb-interceptors$": "<rootDir>/interceptors/index",
      "^@kb-middleware$": "<rootDir>/middleware/index",
      "^@kb-models$": "<rootDir>/models/index",
      "^@kb-organizations$": "<rootDir>/organizations/index",
      "^@kb-pull-requests$": "<rootDir>/pull-requests/index",
      "^@kb-repositories$": "<rootDir>/repositories/index",
      "^@kb-session-user$": "<rootDir>/session-user/index",
      "^@kb-shields$": "<rootDir>/shields/index",
      "^@kb-users$": "<rootDir>/users/index",
      "^@kb-webhooks$": "<rootDir>/webhooks/index",
      "^@kb-systems$": "<rootDir>/systems/index",
      "^@kb-tasks$": "<rootDir>/tasks/index"
    }
  }
}
